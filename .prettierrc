{
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "semi": true,
  "useTabs": false,
  "arrowParens": "avoid",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^([~/|@|@/]+)?helpers/",
    "^([~/|@|@/]+)?routes/|^([~/|@|@/]+)?components/|^([~/|@|@/]+)?locales/|^([~/|@|@/]+)?svg/",
    "^([~/|@|@/]+)?assets/|^([~/|@|@/]+)?favicon/|^([~/|@|@/]+)?styles/",
    "^[~/]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "htmlWhitespaceSensitivity": "strict"
}
